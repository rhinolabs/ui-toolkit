{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"outDir": "./dist",
		"module": "ESNext",
		"resolveJsonModule": false,
		"declaration": true,
		"declarationMap": true,
		"noEmit": false
	},
	"include": ["src/**/*"],
	"exclude": [
		"node_modules",
		"dist",
		"src/cosmos",
		"src/App.tsx",
		"src/main.tsx"
	]
}
