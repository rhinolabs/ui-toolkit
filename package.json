{
	"name": "@rhinolabs/ui-toolkit",
	"version": "1.0.0",
	"description": "An open-source collection of frontend libraries for eliminating repetitive boilerplate in UI development",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"build": "pnpm run build:hooks && pnpm run build:ui && pnpm run build:docs",
		"build:hooks": "pnpm --filter=@rhinolabs/react-hooks run build",
		"build:ui": "pnpm --filter=@rhinolabs/ui run build",
		"build:docs": "pnpm --filter=@rhinolabs/docs run build",
		"dev:docs": "pnpm --filter=@rhinolabs/docs run dev",
		"lint": "pnpm --filter=* run lint",
		"lint:fix": "pnpm --filter=* run lint:fix"
	},
	"keywords": [],
	"author": {
		"name": "Rhinolabs Agency",
		"email": "hello@rhinolabs.agency",
		"url": "https://rhinolabs.agency"
	},
	"license": "MIT",
	"devDependencies": {
		"tsx": "^4.19.2",
    "@types/node": "^24.10.0"
	}
}
